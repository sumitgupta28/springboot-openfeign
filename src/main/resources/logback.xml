<configuration>
    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level [requestId:%mdc{requestId}] %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>



    <root level="DEBUG">
        <appender-ref ref="CONSOLE"/>
    </root>
</configuration>